{
  "name": "bootstrap-11ty-starter",
  "version": "1.0.0",
  "description": "A modern frontend starter with Eleventy and Bootstrap 5",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "sass:build": "sass src/assets/scss/main.scss _site/assets/css/main.css --style=compressed --load-path=node_modules",
    "sass:watch": "sass src/assets/scss/main.scss _site/assets/css/main.css --watch --load-path=node_modules",
    "eleventy:serve": "eleventy --serve",
    "eleventy:build": "eleventy",
    "start": "npm-run-all --parallel sass:watch eleventy:serve",
    "build": "npm-run-all clean sass:build eleventy:build"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "bootstrap",
    "static-site"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "sass": "^1.69.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2"
  }
}
